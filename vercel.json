{
    "builds": [
        {
            "src": "library_1/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "buildCommand": "apt-get update && apt-get install -y libmysqlclient-dev && pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "library_1/wsgi.py"
        }
    ]
}
